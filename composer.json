{
  "name": "roquie/laravel-perpage-resolver",
  "description": "Add a few feature to resolve per_page query parameter globally.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "roquie",
      "email": "roquie0@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.3",
    "illuminate/database": "5.* || ^6 || ^7 || ^8",
    "illuminate/pagination": "5.* || ^6 || ^7 || ^8",
    "illuminate/support": "5.* || ^6 || ^7 || ^8"
  },
  "require-dev": {
    "symfony/var-dumper": "^3 || ^4 || ^5",
    "phpunit/phpunit": "^7 || ^8 || ^9",
    "mockery/mockery": "^1.4",
    "php-coveralls/php-coveralls": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Roquie\\LaravelPerPageResolver\\": "src"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Roquie\\LaravelPerPageResolver\\PerPageResolverServiceProvider"
      ]
    }
  }
}
